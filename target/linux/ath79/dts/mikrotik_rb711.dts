/dts-v1/;
/include/ "ar7241.dtsi"

/ {
    model = "MikroTik RB711";
    compatible = "mikrotik,rb711", "qca,ar7241";

    chosen {
        bootargs = "console=ttyS0,115200";
    };

    memory@0 {
        device_type = "memory";
        reg = <0x00000000 0x04000000>; /* 64MB RAM */
    };

    nand@0 {
        compatible = "qca,nand";
        reg = <0x00000000 0x1000000>;
        nand-ecc-mode = "hw";
        #address-cells = <1>;
        #size-cells = <1>;

        partitions {
            compatible = "fixed-partitions";
            #address-cells = <1>;
            #size-cells = <1>;

            partition@0 {
                label = "u-boot";
                reg = <0x00000000 0x00040000>;
                read-only;
            };

            partition@40000 {
                label = "firmware";
                reg = <0x00040000 0x00fb0000>;
            };

            partition@ff0000 {
                label = "art";
                reg = <0x00ff0000 0x00010000>;
                read-only;
            };
        };
    };
};

&eth0 {
    status = "okay";
};

&wmac {
    status = "okay";
    mtd-mac-address = <&art 0x0>;
};
